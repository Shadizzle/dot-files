// Urxvt Settings
URxvt.scrollBar: false
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 1
URxvt.font: xft:Triplicate T3c:bold:antialias=true:pixelsize=14,\
            xft:Symbola:antialias=true:pixelsize=10
URxvt.letterSpace: -1
URxvt.internalBorder: 36

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.keysym.Control-l: command:\033c

//Rofi Settings
rofi.modi: run,drun
rofi.lines: 5
rofi.bw: 0
rofi.font: Symbola 18
rofi.color-window: argb:ff141516, argb:00000000, argb:00000000
rofi.color-normal: argb:00141516, argb:ffa3a5a5, argb:00212222, argb:00141516, argb:ffffffff
rofi.color-active: argb:ff545657, argb:ffa3a5a5, argb:ff545657, argb:ff545657, argb:ffffffff
rofi.color-urgent: argb:00141516, argb:ffcc6666, argb:ff212222, argb:ffcc6666, argb:ffffffff
